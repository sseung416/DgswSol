<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_add_account"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/addAccountFragment"
        android:name="com.example.a2ndproject.ui.view.fragment.AddAccountFragment"
        android:label="add_account_fragment"
        tools:layout="@layout/add_account_fragment">
        <action
            android:id="@+id/action_addAccountFragment_to_connectThisAccountFragment"
            app:destination="@id/connectThisAccountFragment" />
        <action
            android:id="@+id/action_addAccountFragment_to_createAccountFragment"
            app:destination="@id/createAccountFragment" />
        <action
            android:id="@+id/action_addAccountFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/connectThisAccountFragment"
        android:name="com.example.a2ndproject.ui.view.fragment.ConnectThisAccountFragment"
        android:label="connect_this_account_fragment"
        tools:layout="@layout/connect_this_account_fragment" >
        <action
            android:id="@+id/action_connectThisAccountFragment_to_addAccount1Fragment"
            app:destination="@id/addAccountFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountCheckInfoFragment"
        android:name="com.example.a2ndproject.ui.view.fragment.CreateAccountCheckInfoFragment"
        android:label="create_account_check_info_fragment"
        tools:layout="@layout/create_account_check_info_fragment" >
        <action
            android:id="@+id/action_createAccountCheckInfoFragment_to_addAccount1Fragment"
            app:destination="@id/addAccountFragment" />
        <action
            android:id="@+id/action_createAccountCheckInfoFragment_to_createAccountSuccessFragment"
            app:destination="@id/createAccountSuccessFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountFragment"
        android:name="com.example.a2ndproject.ui.view.fragment.CreateAccountFragment"
        android:label="CreateAccountFragment"
        tools:layout="@layout/create_account_fragment">
        <action
            android:id="@+id/action_createAccountFragment_to_addAccount1Fragment"
            app:destination="@id/addAccountFragment" />
        <action
            android:id="@+id/action_createAccountFragment_to_createAccountCheckInfoFragment"
            app:destination="@id/createAccountCheckInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.a2ndproject.ui.view.fragment.HomeFragment"
        android:label="home_fragment"
        tools:layout="@layout/home_fragment" >
        <action
            android:id="@+id/action_homeFragment_to_addAccountFragment"
            app:destination="@id/addAccountFragment" />
        <action
            android:id="@+id/action_homeFragment_to_holdFragment"
            app:destination="@id/holdFragment" />
    </fragment>
    <fragment
        android:id="@+id/holdFragment"
        android:name="com.example.a2ndproject.ui.view.fragment.HoldFragment"
        android:label="HoldFragment"
        tools:layout="@layout/hold_fragment">
        <action
            android:id="@+id/action_holdFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_holdFragment_to_numberPadMoneyFragment"
            app:destination="@id/numberPadMoneyFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountSuccessFragment"
        android:name="com.example.a2ndproject.ui.view.fragment.CreateAccountSuccessFragment"
        android:label="create_account_success_fragment"
        tools:layout="@layout/create_account_success_fragment" >
        <action
            android:id="@+id/action_createAccountSuccessFragment_to_createAccountCheckInfoFragment"
            app:destination="@id/createAccountCheckInfoFragment" />
        <action
            android:id="@+id/action_createAccountSuccessFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/numberPadPinFragment"
        android:name="com.example.a2ndproject.ui.view.fragment.NumberPadPinFragment"
        android:label="number_pad_pin_fragment"
        tools:layout="@layout/number_pad_pin_fragment" >
        <action
            android:id="@+id/action_numberPadPinFragment_to_numberPadMoneyFragment"
            app:destination="@id/numberPadMoneyFragment" />
        <action
            android:id="@+id/action_numberPadPinFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/numberPadMoneyFragment"
        android:name="com.example.a2ndproject.ui.view.fragment.NumberPadMoneyFragment"
        android:label="NumberPadMoneyFragment"
        tools:layout="@layout/number_pad_money_fragment">
        <action
            android:id="@+id/action_numberPadMoneyFragment_to_holdFragment"
            app:destination="@id/holdFragment" />
        <action
            android:id="@+id/action_numberPadMoneyFragment_to_numberPadPinFragment"
            app:destination="@id/numberPadPinFragment" />
    </fragment>
</navigation>